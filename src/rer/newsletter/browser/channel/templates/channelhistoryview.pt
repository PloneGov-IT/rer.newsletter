<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="rer.newsletter">

      <metal:block metal:fill-slot="style_slot">
      </metal:block>

      <metal:block metal:fill-slot="javascript_head_slot">
      </metal:block>

      <body>
        <metal:main fill-slot="main">
          <metal:main-macro define-macro="main">

            <a class="link-parent" i18n:translate="users_manage" href=""
               tal:attributes="href python:context.absolute_url();">
              Come Back
            </a>

            <div id="content-core">

              <table id="message-table" class="display" cellspacing="0" width="100%">
                <thead>
                  <tr>
                    <th>Message</th>
                    <th>Active Users</th>
                    <th>Send Date</th>
                  </tr>
                </thead>
              </table>

            </div>
          </metal:main-macro>
        </metal:main>
      </body>

</html>
